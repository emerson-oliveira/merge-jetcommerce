@model IList<DomainProduct.Entities.Group>

@if (Model != null && Model.Count() > 0)
{
<div class="ui container topsellers">
        @foreach (var group in Model)
        {
          <header class="section-title">
            @group.Name
            <div class="lines">
              <div class="line"></div>
              <div class="line"></div>
            </div>
          </header>          
            @Html.Action("ProductsByGroup", "Product", new { id = group.IdGroup })
        }
</div>
}
